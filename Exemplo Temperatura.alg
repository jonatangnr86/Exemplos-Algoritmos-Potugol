algoritmo "semnome"
// Função :
// Autor :
// Data : 10/03/2021
// Seção de Declarações
var

temp, c, f : real

inicio

escreval ("Informe a temperatura em Celcius")
leia (temp)

c := temp
f := (1.8*c)+32

escreval ("A temperatura em Celcius é: ", c)
escreval ("A temperatura em Fahrenheit é: ", f)

fimalgoritmo
